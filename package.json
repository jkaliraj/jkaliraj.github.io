    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "cross-env": "^10.1.0",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.1",
    "gh-pages": "^6.3.0",
    "gsap": "^1.19.1",
    "json-loader": "^0.5.4",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.8.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
