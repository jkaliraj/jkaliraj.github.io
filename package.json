{
  "name": "portfolio-template",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://jkaliraj.github.io",
  "description": "Portfolio Template with Animations and Navigation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build": "cross-env NODE_ENV=production webpack --bail --progress --colors",
    "postbuild": "shx rm -rf dist/assets && shx cp -r src/assets dist/assets && shx cp dist/index.html dist/404.html && shx touch dist/.nojekyll",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "license": "MIT",
